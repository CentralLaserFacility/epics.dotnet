    field(NELM, "300")
	field(SCAN, "1 second")
}

record(longin, "$(P)Fault")
{
	field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),$(ADDR),1)FAULT")
	field(SCAN, "I/O Intr")
}
