record(waveform, "$(P)RawData")
{
    field(DTYP, "asynFloat32ArrayIn")
    field(FTVL, "FLOAT")
	field(INP,  "@asyn($(PORT),$(ADDR),1)RAWDATA")
    field(NELM, "7522")
	field(SCAN, "1 second")
}

