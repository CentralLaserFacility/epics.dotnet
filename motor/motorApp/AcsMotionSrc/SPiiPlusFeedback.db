# Feedback records for troubleshooting

record(longin,"$(P)$(M):E_TYPE") {
    field(DESC, "Encoder Type")
    field(DTYP, "asynInt32")
    field(INP,  "@asyn($(PORT),$(ADDR))SPIIPLUS_ENC_TYPE")
    field(PINI, "1")
    field(SCAN, "$(SCAN=2)")
}

