        menu(menuAlarmStat)
        interest(2)
        special(SPC_NOMOD)
        prompt("New Alarm Status")
    }
    field(NSEV, DBF_MENU) {
        interest(2)
        prompt("New Alarm Severity")
        special(SPC_NOMOD)
        menu(menuAlarmSevr)
    }
    field(ACKS, DBF_MENU) {
        menu(menuAlarmSevr)
        interest(2)
        special(SPC_NOMOD)
        prompt("Alarm Ack Severity")
    }
    field(ACKT, DBF_MENU) {
        special(SPC_NOMOD)
        interest(2)
        prompt("Alarm Ack Transient")
        menu(menuYesNo)
        promptgroup("70 - Alarm")
