record(bi, "$(P)StartStop")
{
	field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),0,1)STARTSTOP")
}

record(bi, "$(P)Reset")
{
	field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),0,1)RESET")
}

record(bo, "$(P)Fault")
{
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)FAULT")
	field(SCAN, "I/O Intr")
}

record(bo, "$(P)WarningTemperature")
{
    field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)WARNINGTEMPERATURE")
}

record(bo, "$(P)WarningHighLoad")
{
    field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)WARNINGHIGHLOAD")
}

record(ao, "$(P)StatorFrequency")
{
    field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)STATORFREQUENCY")
}

record(ao, "$(P)ConverterTemperature")
{
    field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)CONVERTERTEMPERATURE")
}

record(ao, "$(P)MotorCurrent")
{
    field(DTYP, "asynFloat64")
	field(OUT,  "@asyn($(PORT),0,1)MOTORCURRENT")
}

record(ao, "$(P)PumpTemperature")
{
    field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)PUMPTEMPERATURE")
}

record(ao, "$(P)CircuitVoltage")
{
    field(DTYP, "asynFloat64")
	field(OUT,  "@asyn($(PORT),0,1)CIRCUITVOLTAGE")
}
