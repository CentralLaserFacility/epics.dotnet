record(bo, "$(P)Running")
{
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),$(ADDR),1)RUNNING")
	field(SCAN, "I/O Intr")
	field(ZNAM,  "OFF")
	field(ONAM,  "ON")
}

record(bi, "$(P)Reset")
{
	field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),$(ADDR),1)RESET")
	field(SCAN, "I/O Intr")
	field(ZNAM,  "NORMAL")
	field(ONAM,  "RESET IN PROGRESS")
}

record(bo, "$(P)Fault")
