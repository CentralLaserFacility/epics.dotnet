record(bo, "$(P)StartStop")
{
	field(DESC, "StartStop")
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)STARTSTOP")
	field(ZNAM,  "OFF")
	field(ONAM,  "ON")
}

record(bo, "$(P)Reset")
{
	field(DESC, "Reset")
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),0,1)RESET")
	field(ZNAM,  "NORMAL")
	field(ONAM,  "RESET IN PROGRESS")
}

record(bi, "$(P)Fault")
{
	field(DESC, "Fault")
	field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),0,1)FAULT")
	field(SCAN, "5 second")
	field(PINI, "1")
	field(ZNAM,  "NORMAL")
	field(ONAM,  "FAULT")
}

