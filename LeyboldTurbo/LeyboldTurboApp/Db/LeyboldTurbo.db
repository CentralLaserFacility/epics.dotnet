record(bi, "$(P)Fault")
{
	field(DTYP, "asynUInt32Digital")
	field(INP,  "@asynMask($(PORT),0,1,1)FAULT")
#	field(SCAN, "5 second")
	field(PINI, "1")
}

record(bi, "$(P)WarningTemperature")
{
    field(DTYP, "asynUInt32Digital")
	field(INP,  "@asynMask($(PORT),0,1,1)WARNINGTEMPERATURE")
}

record(bi, "$(P)WarningHighLoad")
{
    field(DTYP, "asynUInt32Digital")
	field(INP,  "@asynMask($(PORT),0,1,1)WARNINGHIGHLOAD")
}

record(ai, "$(P)StatorFrequency")
{
    field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),0,1)STATORFREQUENCY")
}

record(ai, "$(P)ConverterTemperature")
