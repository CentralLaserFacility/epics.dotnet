record(bo, "$(P)Running")
{
	field(DESC, "Running")
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),$(ADDR),1)RUNNING")
	field(SCAN, "I/O Intr")
	field(ZNAM,  "OFF")
	field(ONAM,  "ON")
}

record(bo, "$(P)Reset")
{
	field(DESC, "Reset")
	field(DTYP, "asynInt32")
	field(OUT,  "@asyn($(PORT),$(ADDR),1)RESET")
	field(HIGH, "1.0")
	field(PINI, "1")
	field(ZNAM,  "NORMAL")
	field(ONAM,  "RESET IN PROGRESS")
}

record(bi, "$(P)Fault")
{
	field(DESC, "Fault")
	field(DTYP, "asynInt32")
	field(INP,  "@asyn($(PORT),$(ADDR),1)FAULT")
	field(SCAN, "5 second")
	field(PINI, "1")
	field(ZNAM,  "NORMAL")
	field(ONAM,  "FAULT")
}

